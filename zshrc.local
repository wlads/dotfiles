# Virtualenv/VirtualenvWrapper
# export WORKON_HOME=~/.virtualenvs
# export VIRTUALENVWRAPPER_PYTHON=/usr/local/bin/python3
# source /usr/local/bin/virtualenvwrapper.sh
#
# export ANDROID_SDK_ROOT=/Users/wladimir/Library/Android/sdk
# export ANDROID_HOME=/Users/wladimir/Library/Android/sdk

# GoLang
export GOPATH=/Users/wladimir/Projects/gocode
export PATH=$PATH:$GOPATH/bin

# Java
export JAVA_HOME=$(/usr/libexec/java_home)

# Scripts
export PATH=$PATH:~/Projects/Scripts/Util

export PATH="$HOME/.bin:$PATH"
source /Users/wladimir/.asdf/asdf.sh

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/wladimir/Tools/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/wladimir/Tools/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/wladimir/Tools/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/wladimir/Tools/google-cloud-sdk/completion.zsh.inc'; fi
